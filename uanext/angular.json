{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "uanext": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/uanext",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss",
              "./node_modules/font-awesome/css/font-awesome.css", 
              "./node_modules/maptalks/dist/maptalks.css",
              "./node_modules/nouislider/distribute/nouislider.min.css"
            ],
            "scripts": [
              "./src/js/three.min.js",
              "./src/js/maptalks.min.js",
              "./src/js/maptalks.three.min.js",
              "./src/js/DDSLoader.js",
              "./src/js/OBJLoader.js",
              "./src/js/stats.min.js",
              "./src/js/MTLLoader.js",
              "./src/js/maptalks.markercluster.min.js",
              "./src/js/CSS2DRenderer.js",
              "./src/js/slidePage.min.js",
              "./src/js/d3.v4.min.js",
              "./src/js/mime-types.js",
              "./src/js/LoaderCommons.js",
              "./src/js/LoaderBuilder.js",
              "./src/js/LoaderWorkerSupport.js",
              "./src/js/OBJLoader2.js",
              "./node_modules/nouislider/distribute/nouislider.min.js",
              "./node_modules/three-ziploader/build/ziploader.min.js"
            ]
          },
          "configurations": {
            "master": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.master.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "prod": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "uanext:build"
          },
          "configurations": {
            "prod": {
              "browserTarget": "uanext:build:prod"
            },
            "master": {
              "browserTarget": "uanext:build:master"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "uanext:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.scss",
              "./node_modules/font-awesome/css/font-awesome.css", 
              "./node_modules/maptalks/dist/maptalks.css",
              "./node_modules/nouislider/distribute/nouislider.min.css"
            ],
            "scripts": [
              "./src/js/three.min.js",
              "./src/js/maptalks.min.js",
              "./src/js/maptalks.three.min.js",
              "./src/js/DDSLoader.js",
              "./src/js/OBJLoader.js",
              "./src/js/stats.min.js",
              "./src/js/MTLLoader.js",
              "./src/js/maptalks.markercluster.min.js",
              "./src/js/CSS2DRenderer.js",
              "./src/js/slidePage.min.js",
              "./src/js/d3.v4.min.js",
              "./src/js/mime-types.js",
              "./src/js/mime-types.js",
              "./src/js/LoaderCommons.js",
              "./src/js/LoaderBuilder.js",
              "./src/js/LoaderWorkerSupport.js",
              "./src/js/OBJLoader2.js",
              "./node_modules/nouislider/distribute/nouislider.min.js",
              "./node_modules/three-ziploader/build/ziploader.min.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "uanext-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "uanext:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "uanext:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "uanext",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}